{
    "MDBootstrap Basic Breadcrumb": {
        "prefix": "!mdb-breadcrumb",
        "body": [
            "<nav aria-label=\"breadcrumb\">",
            "   <ol class=\"breadcrumb\">",
            "       <li class=\"breadcrumb-item\"><a href=\"#\">${1:Home}</a></li>",
            "       <li class=\"breadcrumb-item\"><a href=\"#\">${2:First}</a></li>",
            "       <li class=\"breadcrumb-item active\">${3:Second}</li>",
            "   </ol>",
            "</nav>"
        ],
        "description": "Creates MDbootstrap Basic Breadcrumb"
    }
    
}